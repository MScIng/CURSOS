        "# Generar datos simulados\n",
        "np.random.seed(42)\n",
        "horas_de_uso = np.linspace(100, 1000, 100)  # 100 puntos entre 100 y 1000 horas\n",
        "desgaste = 0.05 * horas_de_uso**2 - 20 * horas_de_uso + 1500 + np.random.normal(0, 5000, 100)\n",
        "\n",
        "# Crear un DataFrame con los datos simulados\n",
        "datos = {\n",
        "    'Horas_de_uso': horas_de_uso,\n",
        "    'Desgaste': desgaste\n",
        "}\n",
        "\n",
        "df = pd.DataFrame(datos)\n",
        "\n",
        "# Guardar los datos en un archivo Excel\n",
        "archivo = 'modulo03_regresionLineal.xlsx'\n",
        "df.to_excel(archivo, index=False)"
