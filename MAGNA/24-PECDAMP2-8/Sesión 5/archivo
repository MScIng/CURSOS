      "cell_type": "code",
      "source": [
        "# Importar librerías necesarias\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "from sklearn.datasets import load_iris\n",
        "from sklearn.model_selection import train_test_split, cross_val_score\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "from sklearn.tree import DecisionTreeClassifier\n",
        "from sklearn.ensemble import RandomForestClassifier\n",
        "from sklearn.svm import SVC\n",
        "\n",
        "# Cargar un conjunto de datos (usaremos el conjunto de datos Iris como ejemplo)\n",
        "data = load_iris()\n",
        "X = data.data\n",
        "y = data.target\n",
        "\n",
        "# Definir los modelos a evaluar\n",
        "models = {\n",
        "    'Regresión Logística': LogisticRegression(max_iter=200),\n",
        "    'Árbol de Decisión': DecisionTreeClassifier(),\n",
        "    'Bosque Aleatorio': RandomForestClassifier(),\n",
        "    'Máquina de Soporte Vectorial': SVC(probability=True)\n",
        "}\n",
        "\n",
        "# Evaluar cada modelo usando K-fold cross validation\n",
        "results = {}\n",
        "for name, model in models.items():\n",
        "    scores = cross_val_score(model, X, y, cv=5)  # 5-fold cross validation\n",
        "    results[name] = scores\n",
        "    print(f'{name}: Media = {scores.mean():.3f}, Desviación estándar = {scores.std():.3f}')\n",
        "\n",
        "# Comparar el rendimiento de los modelos\n",
        "best_model = max(results, key=lambda x: results[x].mean())\n",
        "print(f'\\nEl mejor modelo es: {best_model} con una precisión media de {results[best_model].mean():.3f}')\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "WgVmgl0YXZkB",
        "outputId": "d037ff60-4b3a-4734-e6b0-531079525935"
      },
      "execution_count": null,
