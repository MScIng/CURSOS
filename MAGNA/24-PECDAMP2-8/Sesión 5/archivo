      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "# Generar datos simulados\n",
        "np.random.seed(42)\n",
        "n_samples = 500\n",
        "\n",
        "# Variables independientes\n",
        "temperature = np.random.normal(75, 10, n_samples)  # Temperatura media con desviación estándar\n",
        "vibration = np.random.normal(0.5, 0.1, n_samples)  # Vibración promedio con desviación estándar\n",
        "\n",
        "# Variable dependiente (Failure) basada en una lógica simple\n",
        "failure = (temperature > 85) | (vibration > 0.6)  # Fallo si la temperatura es alta o la vibración es elevada\n",
        "failure = failure.astype(int)  # Convertir booleanos a enteros (0 o 1)\n",
        "\n",
        "# Crear un DataFrame\n",
        "data = {\n",
        "    'Temperature': temperature,\n",
        "    'Vibration': vibration,\n",
        "    'Failure': failure\n",
        "}\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "# Guardar en un archivo Excel\n",
        "file_name = 'modulo03_datos_fallo.xlsx'\n",
        "df.to_excel(file_name, index=False)"
      ],
      "metadata": {
        "id": "A7412ftnnf59"
      },
      "execution_count": null,
      "outputs": []
