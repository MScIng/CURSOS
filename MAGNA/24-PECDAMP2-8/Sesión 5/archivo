    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.linear_model import LinearRegression\n",
        "from sklearn.preprocessing import PolynomialFeatures\n",
        "from sklearn.metrics import mean_squared_error, r2_score\n",
        "\n",
        "# Crear un conjunto de datos simulado con un comportamiento no lineal (desgaste acelerado)\n",
        "np.random.seed(42)\n",
        "X = np.linspace(0, 10, 100).reshape(-1, 1)\n",
        "y = 0.5 * X**2 - 2 * X + 3 + np.random.normal(0, 1, X.shape)\n",
        "\n",
        "# ===========================\n",
        "# 1. Gráfico de regresión lineal\n",
        "# ===========================\n",
        "linear_model = LinearRegression()\n",
        "linear_model.fit(X, y)\n",
        "y_pred_linear = linear_model.predict(X)\n",
        "\n",
        "plt.figure(figsize=(12, 8))\n",
        "plt.subplot(2, 2, 1)\n",
        "plt.scatter(X, y, color='blue', label='Datos reales')\n",
        "plt.plot(X, y_pred_linear, color='red', label='Ajuste lineal')\n",
        "plt.title(f'Regresión Lineal\\nR² = {r2_score(y, y_pred_linear):.2f}')\n",
        "plt.xlabel('Tiempo de uso')\n",
        "plt.ylabel('Desgaste')\n",
        "plt.legend()\n",
        "\n",
        "# ===========================\n",
        "# 2. Gráfico de regresión polinómica\n",
        "# ===========================\n",
        "poly = PolynomialFeatures(degree=2)\n",
        "X_poly = poly.fit_transform(X)\n",
        "\n",
        "poly_model = LinearRegression()\n",
        "poly_model.fit(X_poly, y)\n",
        "y_pred_poly = poly_model.predict(X_poly)\n",
        "\n",
        "plt.subplot(2, 2, 2)\n",
        "plt.scatter(X, y, color='blue', label='Datos reales')\n",
        "plt.plot(X, y_pred_poly, color='green', label='Ajuste polinómico (grado 2)')\n",
        "plt.title(f'Regresión Polinómica\\nR² = {r2_score(y, y_pred_poly):.2f}')\n",
        "plt.xlabel('Tiempo de uso')\n",
        "plt.ylabel('Desgaste')\n",
        "plt.legend()\n",
        "\n",
        "# ===========================\n",
        "# 3. Gráfico de residuales para regresión lineal\n",
        "# ===========================\n",
        "residuals_linear = y - y_pred_linear\n",
        "\n",
        "plt.subplot(2, 2, 3)\n",
        "plt.scatter(X, residuals_linear, color='purple', label='Residuales lineales')\n",
        "plt.axhline(y=0, color='red', linestyle='--')\n",
        "plt.title('Residuales Regresión Lineal')\n",
        "plt.xlabel('Tiempo de uso')\n",
        "plt.ylabel('Residual')\n",
        "plt.legend()\n",
        "\n",
        "# ===========================\n",
        "# 4. Gráfico de residuales para regresión polinómica\n",
        "# ===========================\n",
        "residuals_poly = y - y_pred_poly\n",
        "\n",
        "plt.subplot(2, 2, 4)\n",
        "plt.scatter(X, residuals_poly, color='orange', label='Residuales polinómicos')\n",
        "plt.axhline(y=0, color='red', linestyle='--')\n",
        "plt.title('Residuales Regresión Polinómica')\n",
        "plt.xlabel('Tiempo de uso')\n",
        "plt.ylabel('Residual')\n",
        "plt.legend()\n",
        "\n",
        "# Mostrar las gráficas\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
