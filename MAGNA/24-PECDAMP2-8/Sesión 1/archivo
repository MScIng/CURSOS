e datos de sensores\n","import numpy as np\n","\n","# Crear datos simulados\n","np.random.seed(0)\n","temperatura = np.random.normal(loc=75, scale=5, size=100)  # Temperaturas en grados Fahrenheit\n","presion = np.random.normal(loc=30, scale=2, size=100)  # Presiones en psi\n","fallos = (temperatura > 80) & (presion < 28)  # Condición simplificada para fallo\n","\n","# Crear DataFrame\n","datos_sensores = pd.DataFrame({\n","    'temperatura': temperatura,\n","    'presion': presion,\n","    'fallo': fallos\n","})\n","\n","# Modificar la impresión del conteo de fallos\n","conteo_fallos_sensores = datos_sensores['fallo'].value_counts()\n","\n","print(\"Conteo de fallos por condiciones de sensores:\")\n","for valor, conteo in conteo_fallos_sensores.items():\n","    estado = \"Fallo\" if valor else \"Sin Fallo\"\n","    print(f\"{estado}: {conteo}\")\n","\n","\n","# Visualización\n","datos_sensores[datos_sensores['fallo']].plot.scatter(x='temperatura', y='presion', color='red', label='Fallo')\n","datos_sensores[~datos_sensores['fallo']].plot.scatter(x='temperatura', y='presion', color='blue', label='Sin Fallo')\n","plt.title('Análisis de Datos de Sensores')\n","plt.xlabel('Temperatura')\n","plt.ylabel('Presión')\n","plt.grid(True)\n","plt.legend()\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":956},"id":"fD2JFJUiwyf7","outputId":"68f28ab8-01f0-406e-cfbc-ff3bfb887a38"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Conteo de fallos por condiciones de sensores:\n","Sin Fallo: 97\n","Fallo: 3\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["import numpy as np\n","import matplotlib.pyplot as plt\n","\n","# Simulación de Montecarlo para escenarios de fallos\n","np.random.seed(42)\n","n_simulaciones = 1000\n","horas_operacion = 1000\n","\n","# Distribución de tiempo hasta fallos (ej: distribución normal)\n","tiempos_fallos = np.random.normal(loc=500, scale=100, size=n_simulaciones)\n","\n","# Filtrar fallos que ocurren dentro del tiempo de operación\n","fallos_validos = tiempos_fallos[tiempos_fallos <= horas_operacion]\n","\n","# Escenario: Mantenimiento cada 300 horas\n","mantenimiento_intervalo = 300\n","fallos_con_mantenimiento = np.count_nonzero(fallos_validos > mantenimiento_intervalo)\n","\n","print(f\"Escenario: {fallos_con_mantenimiento} fallos detectados con mantenimiento cada {mantenimiento_intervalo} horas.\")\n","\n","# Visualización del escenario de mantenimiento\n","plt.hist(fallos_validos, bins=30, alpha=0.7, label=\"Fallos sin mantenimiento\")\n","plt.axvline(mantenimiento_intervalo, color='red', linestyle='dashed', linewidth=2, label=\"Intervalo de Mantenimiento\")\n","plt.title('Simulación de Montecarlo: Fallos vs Mantenimiento')\n","plt.xlabel('Horas de Operación')\n","plt.ylabel('Frecuencia de Fallos')\n","plt.legend()\n","plt.show()\n"],"metadata":{"id":"O3RbpE7X9ERa","outputId":"4c4ff86a-2a48-4f88-9039-14c4eeaee543","colab":{"base_uri":"https://localhost:8080/","height":489}},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Escenario: 983 fallos detectados con mantenimiento cada 300 horas.\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]}]}