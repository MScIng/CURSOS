grid_search.best_params_)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"7YeWSDlIbzhp","executionInfo":{"status":"ok","timestamp":1732471847115,"user_tz":300,"elapsed":50953,"user":{"displayName":"MARIO ANDERSON GUTIERREZ TERRONES","userId":"12277729976661677662"}},"outputId":"cd1d84c4-4698-4e2b-ecfb-2310a20e43f1"},"execution_count":6,"outputs":[{"output_type":"stream","name":"stdout","text":["Fitting 5 folds for each of 144 candidates, totalling 720 fits\n","Mejores parámetros: {'max_depth': 8, 'min_samples_leaf': 1, 'min_samples_split': 2, 'n_estimators': 80}\n"]}]}]}