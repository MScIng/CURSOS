ll_type":"code","source":["import numpy as np\n","import pandas as pd\n","from sklearn.datasets import make_classification\n","\n","# 1. Generación de datos simulados\n","n_samples = 1200  # Más muestras para este ejemplo\n","n_features = 3    # Velocidad, corriente y nivel de ruido\n","X, y = make_classification(n_samples=n_samples, n_features=n_features, n_informative=2,\n","                           n_redundant=1, n_classes=2, random_state=43)\n","\n","# Convertir a un DataFrame para visualización\n","data = pd.DataFrame(X, columns=['Velocidad', 'Corriente', 'Ruido'])\n","data['Fallo'] = y\n","\n","# Guardar el DataFrame en un archivo Excel\n","data.to_excel('modulo05_datos_rf.xlsx', index=False)\n","\n","print(\"Datos guardados en 'modulo05_datos_rf.xlsx'.\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"spmfDdk1dXWL","outputId":"0cc84f82-ecf7-4a3b-9d68-75101e0ca5b9","executionInfo":{"status":"ok","timestamp":1732929300517,"user_tz":300,"elapsed":464,"user":{"displayName":"Breynner Chavez","userId":"11596173985832807345"}}},"execution_count":12,"outputs":[{"output_type":"stream","name":"stdout","text":["Datos guardados en 'modulo05_datos_rf.xlsx'.\n"]}]},{"cell_type":"code","source":["import numpy as np\n","import pandas as pd\n","from sklearn.datasets import make_classification\n","from sklearn.ensemble import RandomForestClassifier\n","from sklearn.model_selection import train_test_split\n","from sklearn.metrics import classification_report\n","import matplotlib.pyplot as plt\n","\n","# 1. Cargar los datos desde el archivo Excel\n","data = pd.read_excel('modulo05_datos_rf.xlsx')\n","\n","# Separar las características y la etiqueta\n","X = data[['Velocidad', 'Corriente', 'Ruido']]\n","y = data['Fallo']\n","\n","# 2. División de los datos en entrenamiento y prueba\n","X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.3, random_state=42)\n","\n","# 3. Aplicación de Random Forest\n","random_forest = RandomForestClassifier(\n","    n_estimators=40,    # Aumentamos el número de árboles para mayor robustez\n","    max_depth=5,        # Mayor profundidad para capturar relaciones más complejas\n","    min_samples_split=10,  # Mínimo de muestras para dividir un nodo\n","    min_samples_leaf=1,   # Mínimo de muestras en una hoja\n","    random_state=42\n",")\n","random_forest.fit(X_train, y_train)\n","\n","# 4. Predicción\n","y_pred = random_forest.predict(X_test)\n","\n","# 5. Evaluación del modelo\n","print(\"\\nReporte de clasificación:\")\n","print(classification_report(y_test, y_pred))\n","\n","# 6. Visualización de la importancia de las características\n","feature_importances = random_forest.feature_importances_\n","\n","# Mostrar las características más importantes\n","feature_importance_df = pd.DataFrame({\n","    'Característica': ['Velocidad', 'Corriente', 'Ruido'],\n","    'Importancia': feature_importances\n","}).sort_values(by='Importancia', ascending=False)\n","\n","print(\"\\nImportancia de las características:\")\n","print(feature_importance_df)\n","\n","# 7. Mostrar el puntaje de precisión en entrenamiento y prueba\n","print(\"\\nPuntaje de precisión en el conjunto de entrenamiento:\")\n","print(random_forest.score(X_train, y_train))\n","\n","print(\"\\nPuntaje de precisión en el conjunto de prueba:\")\n","print(random_forest.score(X_test, y_test))\n","\n","print(\"\")\n","\n","# Gráfica de importancias\n","plt.figure(figsize=(8, 6)) # Corregido: matplotlib.pyplot ahora se utiliza correctamente\n","plt.bar(feature_importance_df['Característica'], feature_importance_df['Importancia'], color='skyblue')\n","plt.xlabel('Características')\n","plt.ylabel('Importancia')\n","plt.title('Importancia de las características según Random Forest')\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"rw9z2armdizb","outputId":"485e6d30-28f5-4de4-81f2-2be9ef02219d","executionInfo":{"status":"ok","timestamp":1732929735798,"user_tz":300,"elapsed":102